{
  "name": "seth-clinic-backend",
  "version": "1.0.0",
  "description": "Backend API for Seth Medical Clinic Management System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "node dist/worker.js",
    "migrate": "node dist/scripts/migrate.js",
    "seed": "node dist/scripts/seed.js",
    "seed:clinical": "ts-node src/scripts/seedClinicalData.ts",
    "setup:dev": "npm install && npm run build && npm run migrate && npm run seed && npm run seed:clinical",
    "setup:prod": "npm ci && npm run build && npm run migrate && npm run seed && npm run seed:clinical",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "deploy:check": "npm run lint && npm run test && npm run build",
    "health:check": "curl -f http://localhost:5000/health || exit 1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bullmq": "^4.15.0",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "axios": "^1.6.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "pdfkit": "^0.14.0",
    "exceljs": "^4.4.0",
    "mime-types": "^2.1.35",
    "archiver": "^6.0.1",
    "sharp": "^0.32.6",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9",
    "@types/multer": "^1.4.11",
    "@types/speakeasy": "^2.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/nodemailer": "^6.4.14",
    "@types/node": "^20.9.0",
    "@types/pdfkit": "^0.12.12",
    "@types/mime-types": "^2.1.4",
    "@types/archiver": "^6.0.2",
    "@types/sharp": "^0.32.0",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.2.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0"
  }
}
