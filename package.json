{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "lint:backend": "cd backend && npm run lint",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "docker:dev": "docker compose up -d",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:build": "docker compose build --no-cache",
    "docker:build:prod": "docker compose -f docker-compose.prod.yml build --no-cache",
    "docker:down": "docker compose down",
    "docker:down:prod": "docker compose -f docker-compose.prod.yml down",
    "docker:logs": "docker compose logs -f",
    "docker:logs:prod": "docker compose -f docker-compose.prod.yml logs -f",
    "setup:dev": "chmod +x scripts/setup-dev.sh && ./scripts/setup-dev.sh",
    "deploy:prod": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "db:migrate": "cd backend && npm run migrate",
    "db:seed": "cd backend && npm run seed",
    "db:reset": "docker compose down -v && docker compose up -d postgres redis && sleep 10 && npm run db:migrate && npm run db:seed && npm run seed:clinical",
    "seed:clinical": "cd backend && npm run seed:clinical",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prescription:dev": "npm run docker:dev && echo 'Prescription system starting...' && sleep 20 && echo 'Access at: http://localhost:3000/prescriptions'",
    "prescription:build": "npm run docker:build && npm run docker:dev",
    "diagnostics:dev": "npm run docker:dev && echo 'Diagnostics system starting...' && sleep 20 && echo 'Access at: http://localhost:3000/diagnostics'",
    "diagnostics:build": "npm run docker:build && npm run docker:dev",
    "sha:dev": "npm run docker:dev && echo 'SHA system starting...' && sleep 20 && echo 'Access at: http://localhost:3000/sha'",
    "clinical:dev": "npm run docker:dev && npm run seed:clinical && echo 'Clinical autocomplete system ready!' && echo 'Access at: http://localhost:3000'",
    "system:full-setup": "npm run docker:build && npm run docker:dev && sleep 30 && npm run seed:clinical && echo 'Complete system setup finished!'",
    "health:check": "curl -f http://localhost:5000/health && echo 'Backend OK' && curl -f http://localhost:3000/api/health && echo 'Frontend OK'",
    "update:production": "chmod +x scripts/production-update.sh && ./scripts/production-update.sh",
    "deploy:feature": "chmod +x scripts/feature-deployment.sh && ./scripts/feature-deployment.sh",
    "migrate:db": "chmod +x scripts/database-migration.sh && ./scripts/database-migration.sh",
    "system:integrity": "chmod +x scripts/system-integrity-check.sh && ./scripts/system-integrity-check.sh",
    "backup:db": "./scripts/database-migration.sh backup",
    "update:rolling": "./scripts/production-update.sh rolling",
    "update:maintenance": "./scripts/production-update.sh maintenance"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.3.4",
    "@playwright/test": "^1.42.1",
    "@radix-ui/react-accordion": "1.2.2",
